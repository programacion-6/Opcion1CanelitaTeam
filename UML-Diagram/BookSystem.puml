@startuml BookSystem
package "Book System" {
    
    ' Define main interfaces
    
    interface IBookManagement {
        + addBook()
        + updateBook()
        + removeBook()
        + listBooksByGenre()
        + CheckAvailability()
    }
    
    ' Define main classes

    class BookManager implements IBookManagement {
        - BookList : List
        + FoundBookByTitle()
    }

    class Book {
        - title: String
        - author: String
        - ISBN: String
        - genre: String
        - publicationYear: Date
    }

}
@enduml 